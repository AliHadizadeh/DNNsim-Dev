
<transform {
    batch: <pos_num> //Numpy traces batch. Default 0
	network: <Network name>
	inputType: <Caffe|Protobuf|Gzip>
	inputDataType: <Float32|Fixed16>
	outputType: <Protobuf|Gzip>
	outputDataType: <Float32|Fixed16>
	[activate_bias_and_out_act: true]
}>

<simulate {
    batch: <pos_num> //Numpy traces batch. Default 0
    epochs: <pos_num> //Default 1
	network: <Network name>
	inputType: <Caffe|Trace|CParams|Protobuf|Gzip>
	inputDataType: <Float32|Fixed16>
	network_bits: <pos_num> // Default 16b networks
	[activate_bias_and_out_act: true]
	[training: true]
	<experiment {
		architecture: <None|BitPragmatic|Stripes|DynamicStripes|Loom|Laconic|BitTacticalP|BitTacticalE|SCNN|SCNNp|SCNNe|
		                BitFusion>
		task: <Cycles|Potentials|Schedule|AvgWidth> //Schedule only for BitTactical, AvgWidth only for DynamicStripes
		task: <Inference|Sparsity|BitSparsity> //Tasks for None architecture

		[n_columns: <pos_num>] //Default 16
		[n_rows: <pos_num>] //Default 16 (LAC 8)
		[column_registers: <pos_num>] //Default 0
		[precision_granularity: <pos_num>] //Only for Dynamic Stripes and BitTacticalP: Default 256 (Tile)
		[minor_bit: true] //Only for DynamicStripes-like architectures
		[leading_bit: true] //Only for DynamicStripes-like architectures
		[bits_first_stage: <pos_num>] //Only for BitPragmatic and BitTacticalE: Default 0
		[bits_pe: <pos_num>] //Only for Stripes
		[lookahead_d: <pos_num>] //Only for BitTactical: Default 5
		[lookaside_h: <pos_num>] //Only for BitTactical: Default 2
		[search_shape: <L|T>] //Only for BitTactical: Default L
		[read_schedule_from_proto: true] //Only for BitTactical: Default false
		[pe_serial_bits: <pos_num>] //Default 1
		[dynamic_weights: true] //Only for Loom
		[diffy: true] //Only for BitPragmatic and DynamicStripes

		//SCNN-like architectures
		[Ht: <pos_num>] //Default 8 | p and e: 32
		[Wt: <pos_num>] //Default 8 | p and e: 32
		[I: <pos_num>] //Default 4
		[F: <pos_num>] //Default 4
		[out_acc_size: <pos_num>] //Default 1024
		[banks: <pos_num>] //Default 32 (Max 32)

		//BitFusion
		[m: <pos_num>] //Default 16
		[n: <pos_num>] //Default 32
		[PMAX: <pos_num>] //Default 8
		[PMIN: <pos_num>] //Default 2
	}>
}>
